<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="220.0" maxWidth="560.0" minHeight="150.0" minWidth="500.0" prefHeight="220.0" prefWidth="560.0" style="-fx-background-color: #F0F4F8;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.inferface_distribution.ChangeDutyController">
   <children>
      <VBox layoutY="3.0" prefHeight="220.0" prefWidth="560.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <TextField fx:id="textFieldchange" prefHeight="30.0" prefWidth="300.0" promptText="Впишіть причину" style="-fx-background-radius: 15;" />
                  <Button fx:id="btnSend" maxWidth="81.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" style="-fx-background-radius: 15; -fx-background-color: #719FD4;" text="Надіслати" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <ChoiceBox prefHeight="30.0" prefWidth="300.0" />
                  <Button fx:id="btnCancel" onAction="#cancelButton" mnemonicParsing="false" prefHeight="30.0" prefWidth="80.0" style="-fx-background-radius: 15; -fx-background-color: #719FD4;" text="Скасувати" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
