<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@my.css" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.inferface_distribution.ViewDutyController">

    <children>
        <TableView fx:id="dutyTable" layoutX="20.0" layoutY="20.0" prefHeight="520.0" prefWidth="760.0">
            <columns>
                <TableColumn fx:id="monthColumn" prefWidth="150.0" text="Місяць" />
                <TableColumn fx:id="dayColumn" prefWidth="150.0" text="День" />
                <TableColumn fx:id="placeColumn" prefWidth="200.0" text="Місце наряду" />
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
        <Button fx:id="btnBack" layoutX="731.0" layoutY="560.0" onAction="#handleBack" text="Назад" />
    </children>
</AnchorPane>
