<AnchorPane fx:id="rootPane" stylesheets="@my.css" prefHeight="800.0" prefWidth="1440.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.inferface_distribution.AdminviewController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="972.0" layoutY="183.0" prefHeight="551.0" prefWidth="466.0" spacing="50.0">
